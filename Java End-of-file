# Day1
import java.io.*;
import java.util.*;


public class Solution {

    public static void main(String[] args) {
        String n;
        Scanner sc = new Scanner(System.in); 
       String l;
       int i = 1;
       while(sc.hasNext()){
           l = sc.nextLine();
           System.out.println(i +" "+ l);
           i++;
       }
        
    }
}
